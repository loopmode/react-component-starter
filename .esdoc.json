{
  "source": "./src",
  "destination": "./docs",
  "index": "./README.md",
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src/", "to": "lib/"}
        ]
      }
    },
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {"name": "esdoc-jsx-plugin"},
    {"name": "esdoc-react-plugin"}
  ]
}